{
  "phases": {
    "package": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/chart"
        }
      },
      "actions": [
        {
          "action_id": "helm-package"
        }
      ]
    },
    "verify": {
      "context": {
        "runtime_parameters": "{CARTO_RUNTIME_PARAMETERS_STG}",
        "target_platform": {
          "target_platform_id": "{TARGET_PLATFORM}",
          "size": {
            "name": "L8",
            "worker_nodes_instance_count": 2,
            "master_nodes_instance_count": 1
          }
        }
      },
      "actions": [
        {
          "action_id": "goss",
          "params": {
            "resources": {
              "url": "{SHA_ARCHIVE}",
              "path": "/.vib/carto/goss/goss-accounts-www"
            },
            "remote": {
              "workload": "deploy-carto-accounts-www"
            }
          }
        },
        {
          "action_id": "cypress",
          "params": {
            "timeout_minutes": 50,
            "resources": {
              "url": "https://s3.amazonaws.com/bitnami-autobuild-ondemand/temporary/s3temp-bitnami-20220517113455-63538/vib-cypress.tar.gz?AWSAccessKeyId=AKIAJUJS62Y5YSCL4CIA&Expires=1653046495&Signature=LPMZtQphyiqJybYppyAAaJrWr8c=",
              "path": "workspace-www"
            },
            "endpoint": "lb-carto-router-https",
            "env": {
              "RECORD_KEY": "{VIB_ENV_CYPRESS_RECORD_KEY}",
              "FIREBASE_API_KEY": "{VIB_ENV_CYPRESS_FIREBASE_API_KEY}",
              "CLIENT_SECRET": "{VIB_ENV_CYPRESS_CLIENT_SECRET}",
              "MANAGEMENT_API_CLIENT_SECRET": "{VIB_ENV_CYPRESS_MANAGEMENT_API_CLIENT_SECRET}",
              "CYPRESS_CONNECTION_CREDENTIALS": "{VIB_ENV_CYPRESS_CONNECTION_CREDENTIALS}"
            }
          }
        }
      ]
    }
  }
}
