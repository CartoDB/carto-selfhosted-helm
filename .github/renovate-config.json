{
  "branchPrefix": "renovate-dep/",
  "onboarding": false,
  "requireConfig": "optional",
  "platform": "github",
  "forkProcessing": "enabled",
  "repositories": ["CartoDB/carto-selfhosted-helm"],
  "enabledManagers": ["regex"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["manifests/embbeded-cluster.yaml"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s\\S+?:(?<currentValue>\\S+)\\s+\\S+:.+"
      ],
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
    },
    {
      "customType": "regex",
      "managerFilePatterns": ["manifests/embbeded-cluster.yaml"],
      "matchStrings": ["version:\\s*(?<currentValue>\\d+\\.\\d+\\.\\d+\\+k8s-\\d+\\.\\d+)"],
      "depNameTemplate": "replicatedhq/embedded-cluster",
      "datasourceTemplate": "github-tags"
    }
  ]
}
